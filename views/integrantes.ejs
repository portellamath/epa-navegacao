<%- include('partials/head') %>
<%- include('partials/navbar') %>

<main class="container my-4">
  <h1>Integrantes</h1>
  <div class="row">
    <% integrantes.forEach(member => { %>
      <div class="col-md-4 text-center mb-4">
        <img src="<%= member.img %>" alt="<%= member.nome %>" class="img-fluid rounded-circle" style="height:140px; width:140px; object-fit:cover;" />
        <h5 class="mt-2"><%= member.nome %></h5>
        <p class="small text-muted"><%= member.bio %></p>
      </div>
    <% }) %>
  </div>
</main>

<%- include('partials/footer') %>
